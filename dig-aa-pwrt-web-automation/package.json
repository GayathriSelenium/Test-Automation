{
    "name": "Dig-AA-PWRT-Web",
    "version": "1.0.0",
    "description": "",
    "main": "lib/index.js",
    "scripts": {
        "build:all": "webpack --mode development",
        "build:api": "webpack --mode development --config-name api",
        "build:web:dev": "webpack --config-name web --env development",
        "build:web:prod": "webpack --config-name web --env production",
        "build:stats": "webpack --env development --json > ./lib/stats.json",
        "start": "webpack-dev-server --open --mode development",
        "package:web": "aws cloudformation package --template-file ./ops/template-web.yml --output-template-file ./ops/dist/template-web.yml --s3-bucket igshared-resources --s3-prefix Lambda/dev --profile igshared",
        "deploy:web": "aws cloudformation deploy --template-file ./ops/dist/template-web.yml --stack-name Digital-PWRT-Web --parameter-overrides EdgeAuthARN=arn:aws:lambda:us-east-1:930928620228:function:ServerlessAuthEdge EdgeAuthVersion=90 --profile igbuild",
        "upload:web:dev": "aws s3 cp ./lib s3://igbuild-digital-pwrt-application --recursive --exclude '.api/**' --profile igbuild",
        "upload:web:qa": "aws s3 cp ./lib s3://iganp-digital-pwrt-application --recursive --exclude '.api/**' --profile iganp",
        "package:api": "aws cloudformation package --template-file ./ops/template-api.yml --output-template-file ./ops/dist/template-api.yml --s3-bucket igshared-resources --s3-prefix Lambda/dev --profile igshared",
        "deploy:api": "aws cloudformation deploy --template-file ./ops/dist/template-api.yml --stack-name Digital-PWRT-Api --parameter-overrides CognitoId=ap-southeast-2_hjyuB2tyf --s3-bucket igbuild-cf-templates --capabilities CAPABILITY_IAM --profile igbuild",
        "test": "jest --coverage",
        "cypress:open": "cypress open",
        "start:api": "sam local start-api -t ./ops/template-api.yml"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
